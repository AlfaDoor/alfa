<odoo>
    <data>
        <record id="category_contact" model="ir.module.category">
            <field name="name">Contacts</field>
            <field name="sequence">55</field>
        </record>
		<record id="group_contact_user" model="res.groups">
            <field name="name">Contact User</field>
            <field name="category_id" ref="category_contact"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
        <record id="group_contact_manager" model="res.groups">
            <field name="name">Contact Manager</field>
            <field name="category_id" ref="category_contact"/>
            <field name="implied_ids" eval="[(4, ref('group_contact_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
    </data>
</odoo>
